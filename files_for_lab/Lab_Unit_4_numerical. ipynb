    "# Function to differentiate between continuous, discrete and categorical variables"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "id": "91a55e43",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cont_or_disc(customer_df):\n",
    "    discrete = customer_df.select_dtypes(exclude=['float', 'object','datetime'])\n",
    "    continuous = customer_df.select_dtypes('float')\n",
    "    categoricals = customer_df.select_dtypes(include=['object'])\n",
    "    \n",
    "    return continuous, discrete, categoricals\n"
   ]
  },
