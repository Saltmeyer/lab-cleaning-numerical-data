   "source": [
    "# Function to plot for discrete and contiuous variables dataframes\n",
    "def plot_all(df):\n",
    "    for column in df:\n",
    "        sns.displot(x=column, data=df)\n",
    "\n",
    "    return None"
   ]
  },
